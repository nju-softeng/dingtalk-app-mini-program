@import "../common/index.acss";

.rate-index .van-rate {
    -webkit-user-select: none;
    user-select: none;
}

.rate-index .van-rate__item {
    position: relative;
    display: inline-block;
    padding: 0 2px;
}

.rate-index .van-rate__icon {
    display: block;
    height: 1em;
}

.rate-index .van-rate__icon--half {
    position: absolute;
    top: 0;
    left: 2px;
    width: 0.5em;
    overflow: hidden;
}
