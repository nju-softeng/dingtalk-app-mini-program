<view class='sticky-item-index {{className}}' onChildRef='saveChildRef0'>
  <view class='i-sticky-item i-sticky-item-{{index}}'>
    <view class="i-sticky-item-header {{iClass}} {{ isFixed === true ? 'i-sticky-fixed' : '' }}">
      <view class='i-sticky-title'>
        <slot name='title'>
        </slot>
      </view>
    </view>
    <view class='i-index-item-content'>
      <slot name='content'>
      </slot>
    </view>
  </view>
</view>