<view class='tab-bar-index {{className}}' onChildRef='saveChildRef0'>
  <view class="{{iClass}} i-tab-bar {{ fixed ? 'i-tab-bar-fixed' : '' }}">
    <slot>
    </slot>
    <view class='i-tab-bar-list'>
      <view class='i-tab-bar-layer' a:for='{{ list }}' a:key='{{ item.key }}' data-key='{{ item.key }}' style='width: {{ 100 / list.length }}%' ref-numbers='{{ list }}' onTap='handleClickItem'>
      </view>
    </view>
  </view>
</view>